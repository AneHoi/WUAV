<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.VBox?>

<?import java.net.URL?>
<VBox fx:id="vboxBurgerMenu" styleClass="burgerBarView" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.Controller.BurgerBarController">
   <children>
      <FlowPane fx:id="flowHome" alignment="CENTER" minWidth="125.0" prefWidth="125.0">
         <children>
            <ImageView fx:id="imgHome" fitHeight="110.0" fitWidth="100.0" pickOnBounds="true" preserveRatio="true" />
         </children>
         <padding>
            <Insets top="20.0" />
         </padding>
      </FlowPane>
      <FlowPane fx:id="flowCustomers" alignment="CENTER" minWidth="125.0" prefWidth="125.0">
         <children>
            <ImageView fx:id="imgCustomers" fitHeight="110.0" fitWidth="100.0" pickOnBounds="true" preserveRatio="true" />
         </children>
         <padding>
            <Insets top="20.0" />
         </padding>
      </FlowPane>
      <FlowPane fx:id="flowCases" alignment="CENTER" minWidth="125.0" prefWidth="125.0">
         <children>
            <ImageView fx:id="imgCases" fitHeight="110.0" fitWidth="100.0" pickOnBounds="true" preserveRatio="true" />
         </children>
         <padding>
            <Insets top="20.0" />
         </padding>
      </FlowPane>
      <FlowPane fx:id="flowUsers" alignment="CENTER" minWidth="125.0" prefWidth="125.0">
         <children>
            <ImageView fx:id="imgUsers" fitHeight="110.0" fitWidth="100.0" pickOnBounds="true" preserveRatio="true" />
         </children>
         <padding>
            <Insets top="20.0" />
         </padding>
      </FlowPane>
   </children>
   <stylesheets>
      <URL value="@css/Main.css" />
      <URL value="@css/fonts.css" />
   </stylesheets>
</VBox>
